TASK fib n
(
PLEASE PRINT n THANKS
PLEASE VAR one = 1 THANKS
SWITCH MXLTE (n - 2)
    TRUE RESULT one
VAR nn = 0
PLEASE nn = n - 1
VAR nnn = 0 THANKS
PLEASE nnn = n - 2
DO fib nn THANKS
VAR tmp = 0
tmp = GET fib THANKS
DO fib nnn
PLEASE tmp = tmp + GET fib
RESULT tmp THANKS
)
TASK FINDEXIT a
(
PLEASE VAR ten = 10
PLEASE DO fib ten
PRINT GET fib
RESULT a
)